{
  "expo": {
    "name": "nano-SYNAPSYS",
    "slug": "nano-synapsys",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#050f05",
    "ios": {
      "bundleIdentifier": "com.aievolution.nanosynapsys",
      "buildNumber": "1",
      "supportsTablet": false
    },
    "android": {
      "package": "com.aievolution.nanosynapsys",
      "versionCode": 1
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "nano-SYNAPSYS uses Face ID to protect your secure messages."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "nano-SYNAPSYS accesses your photos to send images in chat.",
          "cameraPermission": "nano-SYNAPSYS uses the camera to capture images for chat."
        }
      ]
    ]
  }
}
